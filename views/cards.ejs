        // Load the Google Translate script
        var script = document.createElement('script');
        script.type = 'text/javascript';
        script.src = '//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit';
        document.getElementsByTagName('head')[0].appendChild(script);

        // Apply the stored language
        var storedLanguage = localStorage.getItem('selectedLanguage');
        if (storedLanguage) {
            var intervalId = setInterval(function() {
                var iframe = document.querySelector('iframe.goog-te-banner-frame');
                if (iframe) {
                    clearInterval(intervalId);
                    var selectElement = iframe.contentWindow.document.querySelector('select.goog-te-combo');
                    if (selectElement) {
                        selectElement.value = storedLanguage;
                        selectElement.dispatchEvent(new Event('change'));
                    }
                }
            }, 1000);
        }
    </script>

    

