        <script>
            // let userBalance = "<%= user.account.balance%>"
            // console.log(userBalance);

            function formatUserBalance(userBalance) {
                // Ensure userBalance is treated as a number
                let balance = Number(userBalance);

                // Format the number with commas as thousand separators
                let formattedBalance = balance.toLocaleString();

                return formattedBalance;
                
               
            }
                 
            function formatUserWithdrawal(userBalancee) {
                // Ensure userBalance is treated as a number
                let balance = Number(userBalancee);

                // Format the number with commas as thousand separators
                let formattedBalance = balance.toLocaleString();

                return formattedBalance;
                
               
            }
            

            // Example usage
            let userBalance = document.querySelector('#userBlnce'); 
            console.log(userBalance.innerHTML)
            let userBlnce =userBalance.innerHTML
            // You can change this to test different values
            let formattedBalance = formatUserBalance(userBlnce);
            document.getElementById('balance-display').innerHTML = formattedBalance;
             console.log(formattedBalance);


             let userWithdrawal = document.querySelector('#userWthdl'); 
             let userWthdl = userWithdrawal.innerHTML;
              let formattedWithdrawal = formatUserWithdrawal(userWthdl);
            document.getElementById('withdrawal-display').innerHTML = formattedWithdrawal;

        </script>

